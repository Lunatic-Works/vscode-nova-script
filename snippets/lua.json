    },
    "trans2": {
        "prefix": "trans2",
        "body": [
            "trans2(${1:obj}, '${2:image_name}', ${3:shader_layer}, ${4:times}, ${5:properties}, ${6:times2}, ${7:properties2}, ${8:color2})",
