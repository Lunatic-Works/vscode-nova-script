    },
    "action": {
        "prefix": "action",
        "body": [
            "action(${1:function_name}, ${2:params})",
            "$0"
        ],
        "description": "执行指定函数，第一参数为函数名，之后为函数的参数，比如anim:action(show, bg, 'room')"
    },
    "tint": {
        "prefix": "tint",
        "body": [
            "tint(${1:obj}, ${2:color}, ${3:duration}, ${4:easing})",
            "$0"
        ],
