    },
    "volume": {
        "prefix": "volume",
        "body": [
            "volume(${1:obj}, ${2:value}, ${3:duration})",
            "$0"
        ],
        "description": "动画改变音量"
    },
    "fade_in": {
        "prefix": "fade_in",
        "body": [
            "fade_in(${1:obj}, '${2:audio_name}', ${3:volume}, ${4:duration})",
            "$0"
        ],
        "description": "动画淡入音乐"
    },
    "fade_out": {
        "prefix": "fade_out",
        "body": [
            "fade_out(${1:obj}, ${2:duration})",
            "$0"
        ],
        "description": "动画淡出音乐"
    },
    "wait": {
        "prefix": "wait",
        "body": [
            "wait(${1:duration})",
            "$0"
        ],
        "description": "动画等待"
    },
    "wait_all": {
        "prefix": "wait_all",
        "body": [
            "wait_all(${1:wrap_anim})",
            "$0"
        ],
