        ],
        "description": "设置背景图"
    },
    "bgm": {
        "prefix": "bgm",
        "body": [
            "bgm('${1:audio_name}')",
            "$0"
        ],
        "description": "设置背景音乐"
    },
    "bgm_stop": {
        "prefix": "bgm_stop",
        "body": [
            "bgm_stop()",
            "$0"
        ],
        "description": "停止背景音乐"
    },
    "sound": {
        "prefix": "sound",
        "body": [
            "sound('${1:audio_name}', ${2:position}, ${3:volume})",
            "$0"
        ],
        "description": "播放声音"
    },
    "say": {
        "prefix": "say",
        "body": [
            "say(${1:obj}, '${2:audio_name}', ${3:delay}, ${4:override_auto_voice})",
